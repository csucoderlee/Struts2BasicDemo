<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>


	<package name="sign" extends="struts-default">

		<action name="SignUpAction" class="org.csu.lee.action.sign.SignUpAction">
			<result name="success">/signin.jsp</result>
		</action>
		<action name="SignInAction" class="org.csu.lee.action.sign.SignInAction">
			<result name="success">/main/main.jsp</result>
		</action>

		<action name="UserListAction" class="org.csu.lee.action.admin.UserListAction">
			<result name="success">/admin/userlist.jsp</result>
		</action>
	</package>




</struts>
